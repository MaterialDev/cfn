{
  "name": "@material-dev/cfn",
  "version": "0.15.2",
  "description": "Simple Cloud Formation for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/.bin/xo",
    "tape": "node test | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaterialDev/cfn.git"
  },
  "files": [
    "index.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "xo": {
    "space": 4,
    "rules": {
      "max-lines": [
        "warn",
        500
      ],
      "babel/object-curly-spacing": [
        "error",
        "always"
      ],
      "one-var": "off",
      "operator-linebreak": [
        "error",
        "before"
      ]
    }
  },
  "keywords": [
    "Cloud",
    "Formation",
    "CloudFormation",
    "Cloud-Formation",
    "Node.js",
    "Stack",
    "Deploy"
  ],
  "author": "Andy Day",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaterialDev/cfn/issues"
  },
  "homepage": "https://github.com/MaterialDev/cfn#readme",
  "dependencies": {
    "aws-sdk": "^2.4.0",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "https-proxy-agent": "^1.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "sprintf": "^0.1.5"
  },
  "devDependencies": {
    "blue-tape": "^0.2.0",
    "tap-json": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tap-xunit": "^1.4.0",
    "xo": "^0.16.0"
  }
}
